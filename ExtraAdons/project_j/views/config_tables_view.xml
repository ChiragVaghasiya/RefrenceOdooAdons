<odoo>
    <data>

        <!-- ===============================  TAGS  ============================== -->

        <record id="lekh_tags_form_view" model="ir.ui.view">
            <field name="name">lekh.tags.model.form</field>
            <field name="model">lekh.tags.model</field>
            <field name="arch" type="xml">
                <form string="Tags">
                    <sheet>
                        <group>
                            <group>
                                <field name="lekh_tag_name"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="lekh_tags_tree_view" model="ir.ui.view">
            <field name="name">lekh.tags.model.tree</field>
            <field name="model">lekh.tags.model</field>
            <field name="arch" type="xml">
                <tree string="Tags">
                    <field name="lekh_tag_name"/>
                </tree>
            </field>
        </record>

        <record id="tags_graph_view" model="ir.ui.view">
            <field name="name">lekh.tags.model.graph</field>
            <field name="model">lekh.tags.model</field>
            <field name="arch" type="xml">
                <graph string="Tags Analysis" type="pie">
                </graph>
            </field>
        </record>

        <record id="lekh_tags_action_window" model="ir.actions.act_window">
            <field name="name">Lekh Tags</field>
            <field name="res_model">lekh.tags.model</field>
            <field name="view_mode">tree,form,graph</field>
        </record>


