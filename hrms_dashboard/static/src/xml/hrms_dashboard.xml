<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="HrDashboardMain">
        <div class="oh_dashboards">
            <div class="container-fluid o_hr_dashboard">
            </div>
        </div>
    </t>

    <t t-name="LoginEmployeeDetails">
        <link rel="stylesheet" href="/hrms_dashboard/static/src/css/hrms_dashboard.css"/>
        <div class="row main-section">
            <div class="col-md-2 col-sm-2 dummy"/>
            <div class="col-md-2 col-sm-2 employee-prof">
                <div class="oh-card">
                    <div class="oh-media">
                        <div class="employee-icon">
                            <img class="align-self-center mr-3" alt="" t-att-src="'data:image/png;base64,'+widget.login_employee['image_1920']"/>
                        </div>
                        <div class="media-body employee-name">
                            <h2 class="text-color display-6">
                                <t t-esc="widget.login_employee['name']"/>
                            </h2>
                            <t t-if="widget.login_employee['job_id']">
                                <p class="text-light">
                                    <t t-esc="widget.login_employee['job_id'][1]"/>
                                </p>
                            </t>
                            <t t-if="!widget.login_employee['job_id']">
                                <p class="text-light" style="color:#b6b6b6 !important">Add job title</p>
                            </t>
                        </div>
                        <div class="employee-details">
                            <div class="employee-gender">
                                <t t-if="widget.login_employee['gender'] == 'male'">
                                    <p>
                                        <i class="fa fa-mars"/>
                                    </p>
                                    <p class="text-light">Male</p>
                                </t>
                                <t t-if="widget.login_employee['gender'] == 'female'">
                                    <p>
                                        <i class="fa fa-venus"/>
                                    </p>
                                    <p class="text-light">Female</p>
                                </t>
                                <t t-if="widget.login_employee['gender'] == 'other'">
                                    Other
                                    <p class="text-light">Other</p>
                                </t>
                                <t t-if="!widget.login_employee['gender']">
                                    <p class="text-light" style="color:#b6b6b6 !important">Gender</p>
                                    <p class="text-light" style="color:#b6b6b6 !important">---</p>
                                </t>
                            </div>
                            <div class="employee-age">
                                <t t-if="widget.login_employee['age']">
                                    <p class="text-light">
                                        <t t-esc="widget.login_employee['age']"/>
                                    </p>
                                    <p>Age</p>
                                </t>
                                <t t-if="!widget.login_employee['age']">
                                    <p class="text-light" style="color:#b6b6b6 !important">DOB</p>
                                    <p style="color:#b6b6b6 !important">--/--/----</p>
                                </t>
                            </div>
                            <div class="employee-experience">
                                <t t-if="widget.login_employee['experience']">
                                    <p class="text-light">Joined </p>
                                    <p>
                                        <t t-esc="widget.login_employee['experience']"/>
                                    </p>
                                    <p>Ago</p>
                                </t>
                                <t t-if="!widget.login_employee['experience']">
                                    <p class="text-light" style="color:#b6b6b6 !important">Joined</p>
                                    <p class="text-light" style="color:#b6b6b6 !important">Date</p>
                                    <p class="text-light" style="color:#b6b6b6 !important">--/--/----</p>
                                </t>
                            </div>
                            <div class="employee-country">
                                <t t-if="widget.login_employee['country_id']">
                                    <p class="text-light">Nationality</p>
                                    <p class="text-light">
                                        <t t-esc="widget.login_employee['country_id'][1]"/>
                                    </p>
                                </t>
                                <t t-if="!widget.login_employee['country_id']">
                                    <p class="text-light" style="color:#b6b6b6 !important">Nationality</p>
                                    <p style="color:#b6b6b6 !important">- - -</p>
                                </t>
                            </div>
                            <div class="employee-mobile">
                                <t t-if="widget.login_employee['mobile_phone']">
                                    <p>Mobile</p>
                                    <p class="text-light">
                                        <t t-esc="widget.login_employee['mobile_phone']"/>
                                    </p>
                                </t>
                                <t t-if="!widget.login_employee['mobile_phone']">
                                    <p class="text-light" style="color:#b6b6b6 !important">Mobile</p>
                                    <p style="color:#b6b6b6 !important">- - -</p>
                                </t>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>

    <t t-name="HrDetails">
        <link rel="stylesheet" href="/hrms_dashboard/static/src/css/hrms_dashboard.css"/>

        <!-- ============== GLOBLE VARABLES ============== -->
        <t t-set="is_string" t-value="1"/>
        <t t-set="stages_id_count" t-value="0"/>
        <!-- ============================================= -->

        <div class="main_section_hr">

            <div class="first_section_hr">
                <!-- ***************************** PERSNAL ACIVITY BOXES ******************************* -->
                <t t-set="saprate_activity" t-value="widget.login_employee['saprate_activity']"/>
                <!-- <t t-set="colors" t-value="['#b81a5d','#011d45','#f8bb13','#011d45','#ee7532','#011d45','#b81a5d','#011d45','#d6175c','#011d45','#f8bb13','#011d45','#ee7532']"/> -->
                <!-- <t t-set="temp_color" t-value="0"/> -->

                <t t-foreach="saprate_activity" t-as="i">
                    <div t-attf-class="#{i[0]} first_div_first_section_hr_personal_activity_boxes card text-center">
                        <!-- <div t-attf-class="#{i[0]} first_div_first_section_hr_personal_activity_boxes card text-center" t-attf-style="background-color:#{colors[temp_color]}"> -->
                        <div class="card-body" style="height: 20px;border-radius: 17px;">
                            <!-- <div class="card-body" style="height: 20px;border-radius:41px;"> -->
                            <h5 class="card-title">
                                <!-- <h5 class="card-title" t-attf-style="color:#{colors[temp_color]}"> -->
                                <t t-esc="i[0]"/>
                            </h5>
                            <h4 class="card-number">
                                <!-- <h4 class="card-number" t-attf-style="color:#{colors[temp_color]}"> -->
                                <t t-esc="i[1]"/>
                            </h4>
                        </div>
                    </div>
                    <!-- <t t-set="temp_color" t-value="temp_color+1"/> -->
                </t>
                <!-- <t t-set="temp_color" t-value="0"/> -->


            </div>
            <!-- ******************************************************** -->

            <div class="second_section_hr">
                <!-- **************************  TABLE OF RECRUTMENT ************************** -->
                <t t-set="data_recrutment" t-value="widget.login_employee['loop']"/>
                <t t-set="stages" t-value="widget.login_employee['stages']"/>

                <div class="first_div_second_section_hr_table_recrutment">
                    <table class="table table-borderless" style="margin-top:-3px;">
                        <thead style="background:#71639e">
                            <!-- <tr style = "background-color:lightblue;"> -->
                            <tr>
                                <th scope="col"></th>
                                <t t-foreach = "stages" t-as="i">
                                    <th class="table_head" scope="col">
                                        <t t-esc = "i"/>
                                    </th>
                                </t>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach = "data_recrutment" t-as="i">
                                <tr>
                                    <t t-foreach = "data_recrutment[i]" t-as="j">
                                        <t t-if="is_string == 1">
                                            <td class="bold">
                                                <t t-esc = "data_recrutment[i][j]" />
                                            </td>
                                            <t t-set="is_string" t-value="0"/>
                                        </t>
                                        <t t-else="">
                                            <td t-attf-class="table_data_job_applications #{data_recrutment[i][0] + '_' + stages[stages_id_count]}">
                                                <t t-esc = "data_recrutment[i][j]" />
                                            </td>
                                            <t t-set="stages_id_count" t-value="stages_id_count+1"/>
                                        </t>
                                    </t>
                                </tr>
                                <t t-set="is_string" t-value="1"/>
                                <t t-set="stages_id_count" t-value="0"/>
                            </t>
                        </tbody>
                    </table>

                </div>
                <!-- ********************************************* -->

                <!-- ************************** TABLE OF ACTIVITY ************************** -->
                <t t-if="widget.is_main_hr == true">
                    <t t-set="data_of_activity" t-value="widget.login_employee['dict_of_activity']"/>
                    <t t-set="data_of_activity_types" t-value="widget.login_employee['activity_types']"/>

                    <div class="second_div_second_section_hr_table_activity ">
                        <table class="table table-borderless" style="margin-top:-3px;">
                            <thead style="background:#71639e">
                                <tr>
                                    <th scope="col"></th>
                                    <t t-foreach = "data_of_activity_types" t-as="i">
                                        <th class="table_head" scope="col">
                                            <t t-esc = "i"/>
                                        </th>
                                    </t>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach = "data_of_activity" t-as="i">
                                    <tr>
                                        <t t-foreach = "data_of_activity[i]" t-as="j">
                                            <t t-if="is_string == 1">
                                                <td class="bold">
                                                    <t t-esc = "data_of_activity[i][j]" />
                                                </td>
                                                <t t-set="is_string" t-value="0"/>
                                            </t>
                                            <t t-else="">
                                                <td t-attf-class="table_data_candidates_activity #{data_of_activity[i][0] + '_' + data_of_activity_types[stages_id_count]}">
                                                    <t t-esc = "data_of_activity[i][j]" />
                                                </td>
                                                <t t-set="stages_id_count" t-value="stages_id_count+1"/>
                                            </t>
                                        </t>
                                    </tr>
                                    <t t-set="is_string" t-value="1"/>
                                    <t t-set="stages_id_count" t-value="0"/>
                                </t>
                            </tbody>
                        </table>

                    </div>
                </t>
                <!-- ********************************************* -->
            </div>
            <!-- ********************************************* -->

            <div class="third_section_hr">
                <!-- **************************  TABLE OF TODAYS LEAVE ************************** -->

                <div class="first_div_third_section_hr_table_all_leavest">
                    <t t-set="today_time_off_emp" t-value="widget.login_employee['today_time_off_emp']"/>

                    <table class="table table-borderless" style="margin-top:-3px;">
                        <thead style="background:#71639e">
                            <tr>
                                <th class="table_head" scope="col" style="text-align:left;">Name</th>
                                <th class="table_head" scope="col" style="text-align:left;">Type Of Leave</th>
                                <th class="table_head" scope="col" style="text-align:left;">Today Leave Type</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach ="today_time_off_emp" t-as="i">
                                <tr class="table_data_todays_leave_row" style="text-align:left;">
                                    <t t-foreach="i" t-as="j">
                                        <td t-attf-class="table_data_todays_leave #{i[0] + '_' + i[1]}">
                                            <t t-esc = "j" />
                                        </td>
                                    </t>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>

                <!-- **************************  TABLE OF TODAYS EVENTS ************************** -->
                <div class="second_div_third_section_hr_table_all_events">
                    <t t-set="upcoming_events" t-value="widget.login_employee['upcoming_events']"/>

                    <table class="table table-borderless" style="margin-top:-3px;">
                        <thead style="background:#71639e">
                            <tr>
                                <th class="table_head" scope="col" style="text-align:left;">Name</th>
                                <th class="table_head" scope="col" style="text-align:left;">Event</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach = "upcoming_events" t-as="i">
                                <tr class="table_data_today_event" style="text-align:left;">
                                    <t t-foreach="i" t-as="j">
                                        <td>
                                            <t t-esc = "j" />
                                        </td>
                                    </t>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>


                    <!-- <div class="col-xs-12  hr_notification" style="margin-bottom: 0px;height: 166px;">
                        <div class="col-xs-12" style="padding:0;">
                            <div class="hr_notification_head">
                        Today's Events
                            </div>
                        </div>
                        <div class="col-xs-12 text-align">
                            <t t-foreach="upcoming_events" t-as="event">
                                <div class="col-sm-12 col-lg-12" style="padding:0;">
                                    <div class="text-color">
                                        <div class="">
                                            <div class="media">
                                                <div class="media-body">
                                                    <h2 class="text-color display-6" style="font-size: 15px;">
                                                        <t t-esc="event[0]"/>
                                                    </h2>
                                                    <div style="display:flex; color: black;">
                                                        <div>
                                                            <t t-esc="event[2]"/>
                                                        </div>
                                                        <div style="margin-left: 30px;">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </t>
                        </div>
                    </div> -->



            </div>
            <!-- ********************************************* -->

        </div>
    </t>

