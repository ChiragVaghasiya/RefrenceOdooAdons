<?xml version="1.0"?>
<openerp>
    <data>

        <delete model="hr.holidays.status" search="[('id', 'in',
            [ref('hr_holidays.holiday_status_cl'),ref('hr_holidays.holiday_status_sl'),ref('hr_holidays.holiday_status_comp'),ref('hr_holidays.holiday_status_unpaid')])]"/>

            <!-- Planned leave -->
            <record model="hr.holidays.status" id="holiday_status_pl">
                <field name="name" eval="'Planned leave'"/>
                <field name="sequence">1</field>
                <field name="color_name">black</field>
                <field name="no_leave">1</field>
                <field name="add_interval">monthly</field>
            </record>

            <!-- Unplanned leave -->
            <record model="hr.holidays.status" id="holiday_status_unpl">
                <field name="name">Unplanned Leaves</field>
                <field name="sequence">2</field>
                <field name="color_name">black</field>
                <field name="no_leave">4</field>
                <field name="add_interval">yearly</field>
            </record>

            <!-- Holiday leave -->
            <record model="hr.holidays.status" id="holiday_status_holiday">
                <field name="name">Floating Holiday</field>
                <field name="sequence">3</field>
                <field name="color_name">lavender</field>
                <field name="no_leave">2</field>
                <field name="add_interval">yearly</field>
            </record>

            <!--Maternity Leave-->
            <record model="hr.holidays.status" id="holiday_status_maternity">
                <field name="name">Maternity Leaves</field>
                <field name="sequence">4</field>
                <field name="limit">True</field>
                <field name="color_name">brown</field>
            </record>

            <!--Loss of pay Leave -->
            <record model="hr.holidays.status" id="holiday_status_lop">
                <field name="name">Loss Of Pay</field>
                <field name="sequence">5</field>
                <field name="limit">True</field>
                <field name="color_name">red</field>
            </record>

            <!--Compensation Leave -->
            <record model="hr.holidays.status" id="holiday_status_compensation">
                <field name="name">Compensation Leave</field>
                <field name="sequence">6</field>
                <field name="color_name">black</field>
            </record>

            <!-- Run schedualr notification -->
            <record id="run_schedular" model="mail.template">
                <field name="name">Leave Request</field>
                <field name="model_id" ref="model_hr_holidays_status"/>
                <field name="subject">Execute Add leave schedualr </field>
                <field name="email_from">hr@aspiresoftware.in</field>
                <field name="email_to">hr@aspiresoftware.in</field>
                <field name="user_signature" eval="0"/>
                <field name="body_html"><![CDATA[<p>Hi,</p>
                <p> Leave grant scheduler has been executed successfully. Kindly check leave balance(s). Please contact system administrator in case of any issues.</p>

                <p>Regards,<br/>
                Human Resource</p>
                ]]></field>
            </record>
