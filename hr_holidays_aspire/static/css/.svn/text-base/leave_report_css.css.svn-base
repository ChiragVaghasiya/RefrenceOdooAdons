table {
	border-collapse: collapse;
	width:100%;
	margin-top: 15px;
}

table, td,th{
	border: 1px solid black;
	border-collapse: collapse;
	text-align: center;
	text-align:center;
	font-style: normal;
	font-family: "Proxima Nova";
}
th{
	background-color:LightGray;
	height:18px;
	padding-top:15px;
	padding: 5px; 
	font-size: 18px;
}
td{
	padding-top: 5px;
	padding-left: 10px;
	padding-right: 5px;
	font-size: 16px;
}
tr { page-break-inside: avoid }